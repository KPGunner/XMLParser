import csv, time
import os
import re

start_time = time.time()

path = r'C:\Users\kgunn\PycharmProjects\xml_parse_test\Test'
# new_dir = os.mkdir(os.path.join(path, 'Updates'))
global_file = 'globals.csv'

files = {}
for filename in os.listdir(path):
    if filename.endswith('.xml'):
        file_contents = open(path + "\\" + filename, 'r', encoding='utf-8').readlines()
        files[filename] = file_contents

with open(global_file, 'r') as f:
    # f = f.read()
    f.seek(0)
    reader = csv.reader(f)
    next(reader, None)
    for row in reader:
        print(row[0], row[1])
